   "cell_type": "code",
   "execution_count": 11,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "var('r',positive=True) #introducimos la suposicion r>0\n",
    "x=[0,1,1+r]#posiciones\n",
    "c=sum([m[j]*x[j]/sum(m) for j in range(3)]) #centro masas\n",
    "q=[(h-c).simplify() for h in x] #coordenadas varicentricas\n",
    "U=sum([ sum([m[i]*m[j]/abs(x[i]-x[j]) for i in range(j) ]) for j in range(3)]) #potencial\n",
    "Q=[m[i]**2/abs(q[i]) for i in range(3)]\n"
   ]
